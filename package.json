{
  "name": "claude-control",
  "version": "1.0.0",
  "description": "AI-powered computer control interface using Claude",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build",
    "example": "ts-node src/example.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "lru-cache": "^10.0.0",
    "openai": "^4.82.0",
    "puppeteer": "^22.8.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.17.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "glob": "^10.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "claude",
    "ai",
    "automation",
    "system-control",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/claude-control.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/claude-control/issues"
  }
}
